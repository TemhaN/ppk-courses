<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
  <config>
    <add key="globalPackagesFolder" value="/root/.nuget/packages" />
  </config>
  <fallbackPackageFolders>
    <clear />
  </fallbackPackageFolders>
	<fallbackPackageFolders>
  <add key="VisualStudio" value="C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages" />
	</fallbackPackageFolders>
</configuration>
